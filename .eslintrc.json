{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
      "strict": 0,
      "no-console": 0,
      "prefer-spread": 0,
      "arrow-body-style": 0,
      "no-useless-escape": 0,
      "camelcase": 0,
      "no-await-in-loop": 0,
      "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
      }]
    }
}
