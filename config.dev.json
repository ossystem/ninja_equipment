{
    "cors": {
        "enabled": true,
        "allowedOrigin": "*",
        "allowedMethods": [
            "GET",
            "POST",
            "PUT",
            "OPTIONS",
            "DELETE",
            "PATCH"
        ],
        "allowedHeaders": [
            "Accept",
            "Authorization",
            "Content-Type",
            "Origin",
            "X-Requested-With"
        ]
